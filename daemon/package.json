{
  "name": "@agentbus/daemon",
  "version": "1.0.0",
  "description": "AgentBus跨平台守护进程管理系统",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "agentbus-daemon": "dist/index.js",
    "agentbus-da": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublish": "npm run build",
    "start": "node dist/index.js",
    "daemon": "node dist/index.js daemon",
    "cli": "node dist/index.js cli"
  },
  "keywords": [
    "agentbus",
    "daemon",
    "service",
    "systemd",
    "launchd",
    "windows-service",
    "cross-platform",
    "process-manager",
    "service-manager"
  ],
  "author": "AgentBus Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/agentbus/daemon.git"
  },
  "bugs": {
    "url": "https://github.com/agentbus/daemon/issues"
  },
  "homepage": "https://github.com/agentbus/daemon#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "os": [
    "linux",
    "darwin",
    "win32"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "rimraf": "^3.0.0",
    "typescript": "^4.8.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./daemon": {
      "types": "./dist/daemon.d.ts",
      "require": "./dist/daemon.js",
      "import": "./dist/daemon.js"
    },
    "./service-manager": {
      "types": "./dist/service-manager.d.ts",
      "require": "./dist/service-manager.js",
      "import": "./dist/service-manager.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "require": "./dist/config.js",
      "import": "./dist/config.js"
    },
    "./monitor": {
      "types": "./dist/monitor.d.ts",
      "require": "./dist/monitor.js",
      "import": "./dist/monitor.js"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "require": "./dist/utils.js",
      "import": "./dist/utils.js"
    }
  }
}